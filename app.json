{
  "expo": {
    "name": "vGPT",
    "slug": "vgpt-venice-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vgpt",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/vgpt_favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FAFAFA"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow vGPT to access your photos to attach them to prompts.",
          "cameraPermission": "Allow vGPT to use your camera to take photos for prompts."
        }
      ]
    ],
    "extra": {
      "veniceApiKey": ""
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}